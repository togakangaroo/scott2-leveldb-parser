{
    "rules": {
        "indent": [
            0,
            "tab"
        ],
        "quotes": [
            0,
            "single"
        ],
        "linebreak-style": [
            0,
            "windows"
        ],
        "semi": [
            "off",
            "never"
        ],
      "comma-dangle": [0],
      "no-sparse-arrays": [0]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "2017"
    },
    "globals": {
        "inject": true
    },
    "plugins": [
      "jasmine"
    ]
}
